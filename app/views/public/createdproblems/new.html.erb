<h2>問題新規投稿</h2>
<%= form_with model: @created_problem, url: createdproblems_path, method: :post do |f| %>
  教科</br>
  <%= f.text_field :subject_id %></br>

  問題</br>
  <%= f.text_field :problem %></br>

  模範解答</br>
  <%= f.text_field :model_answer %></br>

  解説</br>
  <%= f.text_field :explanation %></br>

  <%= f.submit "投稿する" %>

<% end %>

