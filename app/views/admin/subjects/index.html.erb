<h2>教科の一覧・編集</h2>

<table class="table table-hover table-inverse">
  <tr>
    <th>ＩＤ</th>
    <th>教科</th>
    <th></th>
  </tr>
  <% @subjects.each do |subject| %>
    <tr>
      <td><%= subject.id %></td>
      <td><%= subject.subject_name %></td>
      <td><%= link_to "編集する", edit_admin_subject_path(subject.id) %></td>
    </tr>
  <% end %>
</table>